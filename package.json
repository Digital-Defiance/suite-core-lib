{
  "name": "@digitaldefiance/suite-core-lib",
  "version": "1.1.42",
  "description": "Generic user system and document system common core for applications",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "npx nx build digitaldefiance-suite-core-lib",
    "test": "npx nx test digitaldefiance-suite-core-lib",
    "lint": "npx nx lint digitaldefiance-suite-core-lib",
    "lint:fix": "npx nx lint digitaldefiance-suite-core-lib --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "format": "npx nx format:write --projects=digitaldefiance-suite-core-lib",
    "format:check": "npx nx format:check --projects=digitaldefiance-suite-core-lib",
    "prepublishOnly": "npx nx build digitaldefiance-suite-core-lib",
    "publish:public": "npm publish --access public"
  },
  "dependencies": {
    "@digitaldefiance/ecies-lib": "1.1.24",
    "@digitaldefiance/i18n-lib": "1.3.13",
    "@digitaldefiance/node-ecies-lib": "1.1.22"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "users",
    "typescript"
  ],
  "author": "Digital Defiance",
  "license": "MIT",
  "packageManager": "yarn@4.10.3"
}
