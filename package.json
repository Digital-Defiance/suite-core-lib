{
  "name": "@digitaldefiance/suite-core-lib",
  "version": "3.10.10",
  "homepage": "https://github.com/Digital-Defiance/suite-core-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Digital-Defiance/suite-core-lib.git"
  },
  "description": "Generic user system and document system common core for applications",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "npx nx build digitaldefiance-suite-core-lib",
    "build:prod": "NODE_ENV=production npx nx build digitaldefiance-suite-core-lib --configuration=production",
    "build:stream": "npx nx build --outputStyle=stream digitaldefiance-suite-core-lib",
    "build:logged": "npx nx build --outputStyle=stream digitaldefiance-suite-core-lib 2>&1 | ansifilter -o build.log",
    "test": "npx nx test digitaldefiance-suite-core-lib",
    "test:stream": "npx nx test --outputStyle=stream digitaldefiance-suite-core-lib",
    "test:logged": "npx nx test --outputStyle=stream digitaldefiance-suite-core-lib 2>&1 | ansifilter -o test.log",
    "test:coverage": "npx jest --coverage --testPathPattern=digitaldefiance-suite-core-lib --coverageReporters=text",
    "lint": "npx nx lint digitaldefiance-suite-core-lib",
    "lint:fix": "npx nx lint digitaldefiance-suite-core-lib --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx}'",
    "format": "npx nx format:write --projects=digitaldefiance-suite-core-lib",
    "format:check": "npx nx format:check --projects=digitaldefiance-suite-core-lib",
    "prepublishOnly": "npx nx build digitaldefiance-suite-core-lib",
    "publish:public": "npm publish --access public"
  },
  "dependencies": {
    "@digitaldefiance/ecies-lib": "4.16.10",
    "@digitaldefiance/i18n-lib": "4.2.2"
  },
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "users",
    "typescript"
  ],
  "author": "Digital Defiance",
  "license": "MIT",
  "packageManager": "yarn@4.10.3",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4"
  }
}
